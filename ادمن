local function getMouseTargetPosition()
    local mouse = player:GetMouse()
    if mouse and mouse.Target and mouse.Hit then
        -- Ù†Ø³ØªØ®Ø¯Ù… mouse.Hit.p Ù„Ù„Ø­ØµÙˆÙ„ Ø¹Ù„Ù‰ Ù…ÙˆØ¶Ø¹ Ø§Ù„Ù†Ù‚Ø± ÙÙŠ Ø§Ù„Ø¹Ø§Ù„Ù…
        return mouse.Hit.p + Vector3.new(0, 1.5, 0) -- Ø¥Ø¶Ø§ÙØ© Ø§Ø±ØªÙØ§Ø¹ Ø¨Ø³ÙŠØ· Ù„ÙŠÙƒÙˆÙ† ÙÙˆÙ‚ Ø§Ù„Ø£Ø±Ø¶
    end
    -- ÙÙŠ Ø­Ø§Ù„Ø© Ø¹Ø¯Ù… Ø§Ù„Ù†Ù‚Ø± Ø¹Ù„Ù‰ Ø´ÙŠØ¡ØŒ Ù†Ø¬Ø¹Ù„Ù‡ ÙŠØ¸Ù‡Ø± Ø£Ù…Ø§Ù… Ø§Ù„Ù„Ø§Ø¹Ø¨
    local root = getRoot()
    if root then
        return root.CFrame.p + root.CFrame.LookVector * 5 + Vector3.new(0, 1.5, 0)
    end
    return nil
end

-- Ø§Ù„Ø¯Ø§Ù„Ø© Ø§Ù„Ù…ÙØ¹Ø¯Ù‘Ù„Ø© Ù„Ø¥Ù†Ø´Ø§Ø¡ Ø§Ù„Ø¹Ù†ØµØ± ÙÙŠ Ù…ÙˆÙ‚Ø¹ Ø§Ù„Ù…Ø§ÙˆØ³ (ÙˆÙ„ÙŠØ³ Ù…Ø±Ø¨ÙˆØ·Ø§Ù‹ Ø¨Ø§Ù„Ù„Ø§Ø¹Ø¨)
local function spawnItemAtMouse(name)
    local pos = getMouseTargetPosition()
    if not pos then return end
    
    local part = Instance.new("Part")
    part.Size = Vector3.new(2, 2, 2)
    part.Shape = Enum.PartType.Block
    part.Anchored = false
    part.CanCollide = true -- ÙŠÙ…ÙƒÙ† Ø£Ù† ÙŠØµØ·Ø¯Ù… Ø¨Ù‡ Ø§Ù„Ù„Ø§Ø¹Ø¨ Ù„Ø¬Ù…Ø¹Ù‡
    part.Material = Enum.Material.Wood -- Ø§Ù„Ø§ÙØªØ±Ø§Ø¶ÙŠ: Ø®Ø´Ø¨
    part.Name = name -- Ø³ÙŠÙƒÙˆÙ† Ø¥Ù…Ø§ "Wood" Ø£Ùˆ "Food"
    
    if name == "Wood" then
        part.Color = Color3.fromRGB(150, 75, 0) -- Ù„ÙˆÙ† Ø¨Ù†ÙŠ
    elseif name == "Food" then
        part.Color = Color3.fromRGB(255, 150, 50) -- Ù„ÙˆÙ† Ø¨Ø±ØªÙ‚Ø§Ù„ÙŠ/Ù„Ø­Ù…ÙŠ
    end
    
    part.CFrame = CFrame.new(pos)
    part.Parent = workspace
    
    -- ÙˆØ¶Ø¹ Ø¹Ù„Ø§Ù…Ø© (tag) Ø¹Ù„Ù‰ Ø§Ù„Ù‚Ø·Ø¹Ø© Ù„ØªØ­Ø¯ÙŠØ¯ Ø£Ù†Ù‡Ø§ Ù…Ø§Ø¯Ø© ÙŠÙ…ÙƒÙ† Ø¬Ù…Ø¹Ù‡Ø§ (Ø§Ø®ØªÙŠØ§Ø±ÙŠ)
    local tag = Instance.new("StringValue", part)
    tag.Name = "ResourceTag"
    tag.Value = name
    
    game.Debris:AddItem(part, 60) -- ÙŠØªÙ… Ø¥Ø²Ø§Ù„ØªÙ‡Ø§ Ø¨Ø¹Ø¯ 60 Ø«Ø§Ù†ÙŠØ©

    return true
end

